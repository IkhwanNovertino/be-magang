<table id="example1" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th style="width: 15%;">Hari/Tanggal</th>
      <th style="width: 20%;">Nama Kegitan</th>
      <th style="width: 50%;">Deskripsi Kegiatan</th>
      <th style="width: 10%;">Status</th>
    </tr>
  </thead>
  <tbody>
    <% logbook.forEach(items => { %>
      <tr>
        <td><%= dateFormatLogbook(items.date) %></td>
        <td><%= items.activity %></td>
        <td><%= items.description %></td>
        <td>
          <% if (items.status === 'success') { %>
            <span class="badge badge-success">Sudah ACC</span>
          <% } else { %>
            <span class="badge badge-warning">Belum ACC</span>
          <% } %>
        </td>
      </tr>
    <% }) %>
  </tbody>
</table>