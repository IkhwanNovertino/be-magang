<table id="example2" class="table table-bordered table-striped">
  <thead>
  <tr>
    <th>No.</th>
    <th>Nama Lengkap</th>
    <th>NIP</th>
    <th>Jabatan</th>
    <th>Aksi</th>
  </tr>
  </thead>
  <tbody>
  <%# supervisor.forEach((e, i) => %>
    <tr>
      <td><#%= i %></td>
      <td><#%= e.name %></td>
      <td><#%= e.name %></td>
      <td><#%= e.name %></td>
      <td>
        <form action="/supervisor/delete/%= e.id %>?_method=DELETE" method="post">
          <a href="/supervisor/edit/%= e.id %>" class="btn btn-default btn-sm">
            <i class="fas fa-pencil-alt"></i>
            Detail
          </a>
          <button class="btn  btn-danger btn-sm">
            <i class="fas fa-trash-alt"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
  <%# }) %>
  </tbody>
</table>