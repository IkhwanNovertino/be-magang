<table id="example2" class="table table-bordered table-striped">
  <thead>
  <tr>
    <th>No.</th>
    <th>Nilai</th>
    <th>Ketegori</th>
    <th>Aksi</th>
  </tr>
  </thead>
  <tbody>
  <% score.forEach((e, i) => {i++ %>
    <tr>
      <td><%= i %></td>
      <td><%= e.title %></td>
      <td>
        <% if (e.category === 'NT') { %>
          Non-Teknis
        <% } else { %>
          Teknis
        <% } %>
      </td>
      <td>
        <form action="/score/delete/<%= e.id %>?_method=DELETE" method="post">
          <a href="/score/edit/<%= e.id %>" class="btn btn-warning btn-sm">
            <i class="fas fa-pencil-alt"></i>
            Ubah
          </a>
          <button class="btn  btn-danger btn-sm">
            <i class="fas fa-trash-alt"></i>
            Hapus
          </button>
        </form>
      </td>
    </tr>
  <% }) %>
  </tbody>
</table>